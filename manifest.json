{
  "name": "Personal Manifesto",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension creates a custom new tab -- one that displays my personal manifesto.",
  "homepage_url": "http://kylechadha.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "chrome_url_overrides": {
    "newtab": "src/override/newtab.html"
  }
}