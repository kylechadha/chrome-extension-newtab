{
  "name": "Better New Tab Page",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "This extension creates a custom new tab. Use it to display a personal manifesto, your daily to do's, or whatever else you'd like!",
  "homepage_url": "http://kylechadha.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "options_page": "src/options/index.html",
  "chrome_url_overrides": {
    "newtab": "src/override/newtab.html"
  }
}