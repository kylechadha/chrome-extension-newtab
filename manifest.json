{
  "name": "A Better New Tab Page",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "This extension creates a beautiful, customizable new tab page.",
  "homepage_url": "http://kylechadha.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options/index.html",
  "chrome_url_overrides": {
    "newtab": "src/override/newtab.html"
  }
}